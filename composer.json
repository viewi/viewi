{
    "name": "viewi/viewi",
    "description": "Powerful tool for building progressive and completely reactive web applications",
    "keywords": [
        "viewi",
        "UI",
        "reactive",
        "template"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-zlib": "*",
        "ext-curl": "*",
        "ext-ctype": "*",
        "nikic/php-parser": "^4.16"
    },
    "autoload": {
        "psr-4": {
            "Viewi\\": "src/Viewi/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Components\\": "server/viewi-app/Components/"
        }
    },
    "bin": [
        "bin/viewi"
    ],
    "authors": [
        {
            "name": "Ivan Voitovych",
            "email": "col606v@gmail.com"
        }
    ],
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*"
    }
}